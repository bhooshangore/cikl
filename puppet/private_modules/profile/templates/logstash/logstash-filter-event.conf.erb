filter {
  if ([type] == 'event') {
    if ([detect_time]) {
      date {
        match => [ "detect_time", "UNIX", "ISO8601" ]
        target => "detect_time"
        timezone => "UTC"
      }
    }
    if ([import_time]) {
      date {
        match => [ "import_time", "UNIX", "ISO8601" ]
        target => "import_time"
        timezone => "UTC"
      }
    }

    if ([type] == 'event') {
      cikl_event_normalize { }

      generate_dns_queries { }

      cikl_mongodb {
        database => 'cikl'
        uri => 'mongodb://localhost:27017'
      }
    }
    
  }
}
