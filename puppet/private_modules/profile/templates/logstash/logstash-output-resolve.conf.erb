output { 
  if ([type] == 'do_dns_query') {
    rabbitmq {
      host => "<%= @rabbitmq_host %>"
      port => <%= @rabbitmq_port %>
      user => "<%= @rabbitmq_username %>"
      password => "<%= @rabbitmq_password %>"
      vhost => "<%= @rabbitmq_vhost %>"
      exchange => ""
      exchange_type => "topic"
      key => "cikl.resolve_fqdn"
    }
  }
}

